#loading library#
require("ggplot2")#
#
# selecting the current directory#
setwd("<directory>")#
#
# read csv file#
data <- read.csv("<output_file_name>")#
setEPS()#
postscript("<eps_image_file_name>")#
#
chart <- ggplot() + #
    ggtitle("IR methods - precision x recall") +#
    labs(x="Precision", y="Recall") +#
    theme(axis.title.y = element_text(size=14, family="Trebuchet MS", color="#666666")) +#
    theme(axis.title.x = element_text(size=14, family="Trebuchet MS", color="#666666")) +#
    theme(axis.text = element_text(size=16, family="Trebuchet MS")) +#
    theme(plot.title = element_text(size=26, family="Trebuchet MS", face="bold", hjust=0, color="#666666"))#
#
# convert factor to numeric for convenience #
data$method_index <- as.numeric(data$method)#
n_methods <- max(data$method_index)#
colors <- rainbow(n_methods)#
#
for (i in 1:n_methods) { #
  method <- subset(data, method_index==i)#
  chart <- chart + geom_line(data = method, aes(x = method$recall, y = method$precision, color = colors[i]))#
}#
#
dev.off()
